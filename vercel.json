{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }